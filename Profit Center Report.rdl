<Report xmlns="http://schemas.microsoft.com/sqlserver/reporting/2016/01/reportdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="SiriusSQLPK_reporting">
      <DataSourceReference>/ReportDataSource</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>ea0d6517-6925-4025-a2b4-11dbce03d002</rd:DataSourceID>
    </DataSource>
    <DataSource Name="Domo_Lookup">
      <DataSourceReference>/Domo Lookup</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>56cd38c9-0a5e-43de-bbbf-e79645ae96c6</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="GatherData">
      <Query>
        <DataSourceName>SiriusSQLPK_reporting</DataSourceName>
        <CommandText>declare @piSalespointGroup int=48

set transaction isolation level read uncommitted 

if object_id('tempdb..#Sales') is not null drop table #Sales
create table #Sales
(
      spGroup         varchar(25),
      sale_no         numeric(17,0),
      invoice_no      numeric(17,0),
      acct_name       varchar(10),
      operator        char(6), 
      salespoint      char(6), 
      firstName       varchar(15), 
      lastName        varchar(20)
)
insert into #Sales
    select sg.descrip, sh.sale_no, sh.invoice_no, sh.acct_name, 
           sh.operator, sh.salespoint, rtrim(op.first_name), rtrim(op.last_name) 
      from sale_hdr sh
      join operator op on op.op_code = sh.operator
      join sp_link sl on sl.salespoint = sh.salespoint
      join sp_group sg on sg.group_no = sl.group_no
     where sh.date_time between '2018-08-14' and '2018-08-14 23:59:59' -- cast(getdate() as date) and getdate()
       and sg.group_no = @piSalespointGroup
-- Faux Finalized sales
insert into #Sales
    select sg.descrip, sh.sale_no, sh.invoice_no, sh.acct_name, 
           sh.operator, sh.salespoint, rtrim(op.first_name), rtrim(op.last_name)
      from sh_save sh
      join operator op on op.op_code = sh.operator
      join sp_link sl on sl.salespoint = sh.salespoint
      join sp_group sg on sg.group_no = sl.group_no
     where sh.date_time between '2018-08-14' and '2018-08-14 23:59:59' --cast(getdate() as date) and getdate()
       and sh.finalized = 0
       and sh.final_pend = 1
       and sg.group_no = @piSalespointGroup

if object_id('tempdb..#Salespoints') is not null drop table #Salespoints
create table #Salespoints
(
      spGroup         varchar(25),
      salespoint      char(6), 
      extension		  decimal(18,2),
	  sale_count	  int
)
insert into #Salespoints
	select sh.spGroup,
           tr.salespoint,
           sum(extension) as extension,
		   count(distinct tr.sale_no) as sale_count   
      from #Sales sh
      join transact tr on tr.sale_no = sh.sale_no
     where tr.department not in ('**TRANS**') 
  group by sh.spGroup, tr.salespoint
  
	union all 
    
	-- Faux Finalize
    select sh.spGroup,
		   ts.salespoint, 
           sum(ts.extension) as extension,
		   count(distinct ts.sale_no) as sale_count
      from #Sales sh
      join tr_save ts on ts.sale_no = sh.sale_no
  group by sh.spGroup, ts.salespoint

-- ===========================================
-- Payments (Faux Finalize payments make it to sale_hdr)
-- ===========================================
select sh.spGroup, 
       cast(rtrim(pm.descrip) as char(25)) + cast(sum(pt.Paid) as char(10)) as PmtString,
	   cast(sum(pt.paid) as numeric(18,2)) as PmtTotal,
       '' as PcString,
       0  as Revenue,
       '' as DiscString,
       0  as DiscountAmount, 
       '' as dcString,
	   0  as dcTotal,
	   0  as taxAmount, 
       '' as itemString,
	   0  as itemTotal, 
	   '' as opString,
	   0  as opTotal,
	   '' as spString,
	   0  as spTotal,
	   '' as voidString,
	   0  as voidTotal  
  from #Sales sh
  join invoices iv on iv.invoice_no = sh.invoice_no
  join siriusv_PaymentTypePaidBySaleNo pt on pt.sale_no = sh.sale_no
  join pmt_type pm on pm.pmtype_no = pt.pmttype_no
 group by
       sh.spGroup, 
       pm.descrip
 
union all 
-- ===========================================
-- Profit Centers
-- ===========================================
select u.spGroup, 
       '' as PmtString,
	   0  as PmtTotal,
       cast(rtrim(pc.descrip) as char(25)) + cast(sum(u.Revenue) as char(10)) as PcString, 
       cast(sum(u.Revenue) as numeric(18,2)) as Revenue, 
       '' as DiscString,
       0  as DiscountAmount, 
       '' as dcString,
	   0  as dcTotal,
	   0  as taxAmount, 
       '' as itemString,
	   0  as itemTotal, 
	   '' as opString,
	   0  as opTotal,
	   '' as spString,
	   0  as spTotal,
	   '' as voidString,
	   0  as voidTotal   
  from (select sh.spGroup,
               pr.pr_ctr_no,
               sum(pr.Revenue) as Revenue
          from #Sales sh
          join transact tr on tr.sale_no = sh.sale_no
          join siriusv_ProfitCenterRevenueByTransNo pr on pr.trans_no = tr.trans_no
          join prof_ctr pc on pc.pr_ctr_no = pr.pr_ctr_no
         group by sh.spGroup, pr.pr_ctr_no
        union all 
        -- Faux Finalize
        select sh.spGroup, 
               pr.pr_ctr_no, 
               sum(pr.Revenue) as Revenue 
          from #Sales sh
          join tr_save ts on ts.sale_no = sh.sale_no
          join (select t.trans_no,
                       case n.number 
                            when 1 then t.pr_ctr_1
                            when 2 then t.pr_ctr_2
                            when 3 then t.pr_ctr_3
                            when 4 then t.pr_ctr_4
                            when 5 then t.pr_ctr_5
                            when 6 then t.pr_ctr_6
                            else null
                       end as pr_ctr_no,
                       case n.number 
                            when 1 then t.pcsplit_1
                            when 2 then t.pcsplit_2
                            when 3 then t.pcsplit_3
                            when 4 then t.pcsplit_4
                            when 5 then t.pcsplit_5
                            when 6 then t.pcsplit_6
                            else null
                       end as Revenue
                  from tr_save t 
                 cross join numbers n
                 where n.number between 1 and 6
                   and case n.number 
                            when 1 then t.pcsplit_1
                            when 2 then t.pcsplit_2
                            when 3 then t.pcsplit_3
                            when 4 then t.pcsplit_4
                            when 5 then t.pcsplit_5
                            when 6 then t.pcsplit_6
                            else null
                       end &lt;&gt; 0) pr on pr.trans_no = ts.trans_no
          join prof_ctr pc on pc.pr_ctr_no = pr.pr_ctr_no
         group by sh.spGroup, pr.pr_ctr_no
        ) u
   join prof_ctr pc on pc.pr_ctr_no = u.pr_ctr_no
  group by u.spGroup, u.pr_ctr_no, pc.descrip

union all 
-- ===========================================
-- Discounts
-- ===========================================
select u.spGroup,
       '' as PmtString,
	   0  as PmtTotal,
       '' as PcString,
       0  as Revenue,
       concat(cast(s.descrip as char(23)),
		space(1),
		replicate(' ',3-len(count(u.special))) + cast(count(u.special) as varchar),
		space(1),
		replicate(' ',7-len(sum(u.DiscountAmount))) + cast(sum(u.DiscountAmount) as varchar)) as DiscString,
       cast(sum(u.DiscountAmount) as numeric(18,2)) as DiscountAmount, 
       '' as dcString,
	   0  as dcTotal,
	   0  as taxAmount,
       '' as itemString,
	   0  as itemTotal,
	   '' as opString,
	   0  as opTotal,
	   '' as spString,
	   0  as spTotal,
	   '' as voidString,
	   0  as voidTotal   
  from (select sh.spGroup,
               tr.special,
               sum(tr.disc_amt) as DiscountAmount
          from #Sales sh
          join transact tr on tr.sale_no = sh.sale_no
         where tr.disc_amt &lt;&gt; 0
         group by sh.spGroup, tr.special
         union all
        -- Faux Finalize
        select sh.spGroup,
               ts.special,
               sum(ts.disc_amt) as DiscountAmount
          from #Sales sh
          join tr_save ts on ts.sale_no = sh.sale_no
         where ts.disc_amt &lt;&gt; 0
         group by sh.spGroup, ts.special
       ) u 
 join specials s on s.name=u.special
 group by u.spGroup, u.special, s.descrip

 union all
-- ===========================================
-- Department/Category Revenue
-- ===========================================
select spGroup,
       '' as PmtString,
	   0  as PmtTotal,
       '' as PcString,
       0  as Revenue,
       '' as DiscString,
       0  as DiscountAmount, 
       concat(category,
			  space(2),
			  replicate(' ', 4-len(sum(qty))) + cast(sum(qty) as varchar),
			  space(3),
			  replicate(' ',5-len(cast(sum(extension)/(select sum(extension) from #Salespoints)*100 as decimal(4,1)))) + cast(cast(sum(extension)/(select sum(extension) from #Salespoints)*100 as decimal(4,1)) as varchar),
			  space(3),
			  replicate(' ',8-len(sum(extension))) + cast(sum(extension) as varchar)
			 ) as dcString, 
       cast(sum(extension) as numeric(18,2)) as dcTotal,
	   sum(tax) as taxAmount,
	   '' as itemString,
	   0  as itemTotal,
       '' as opString,
	   0  as opTotal,
	   '' as spString,
	   0  as spTotal,
	   '' as voidString,
	   0  as voidTotal   
  from (select sh.spGroup,
               tr.department, 
               tr.category,
               sum(tr.quantity) as qty,
			   sum(tr.extension) as extension,
               sum(tr.tax_amount + tr.tax_amt2) as tax,
               count(distinct tr.sale_no) as saleCount, 
               case when sh.acct_name = '*TABLES*' then 1 else 0 end as tableCount
          from #Sales sh
          join transact tr on tr.sale_no = sh.sale_no
         where tr.department not in ('**TRANS**') 
         group by sh.spGroup, department, category, 
                  sh.acct_name
         union all 
        -- Faux Finalize
        select sh.spGroup,
               ts.department, 
               ts.category,
			   sum(ts.quantity) as qty, 
               sum(ts.extension) as extension,
               sum(ts.tax_amount + ts.tax_amt2) as tax,
               count(distinct ts.sale_no) as saleCount, 
               case when sh.acct_name = '*TABLES*' then 1 else 0 end as tableCount
          from #Sales sh
          join tr_save ts on ts.sale_no = sh.sale_no
         group by sh.spGroup, department, category, 
                  sh.acct_name
       ) u
 group by spGroup, department, category

union all
-- ===========================================
-- Item Revenue
-- ===========================================
select spGroup,
       '' as PmtString,
	   0  as PmtTotal,
       '' as PcString,
       0  as Revenue,
       '' as DiscString,
       0  as DiscountAmount,
	   '' as dcString,
	   0  as dcTotal, 
       0  as taxAmount,
	   concat(item,
			  space(2),
			  replicate(' ', 4-len(sum(qty))) + cast(sum(qty) as varchar),
			  space(3),
			  replicate(' ',5-len(cast(sum(extension)/(select sum(extension) from #Salespoints)*100 as decimal(4,1)))) + cast(cast(sum(extension)/(select sum(extension) from #Salespoints)*100 as decimal(4,1)) as varchar),
			  space(3),
			  replicate(' ',8-len(sum(extension))) + cast(sum(extension) as varchar)
			 ) as itemString, 
       cast(sum(extension) as numeric(18,2)) as itemTotal,
       '' as opString,
	   0  as opTotal,
	   '' as spString,
	   0  as spTotal,
	   '' as voidString,
	   0  as voidTotal   
  from (select sh.spGroup,
               tr.item, 
               sum(tr.quantity) as qty,
			   sum(tr.extension) as extension
          from #Sales sh
          join transact tr on tr.sale_no = sh.sale_no
         where tr.department not in ('**TRANS**') 
         group by sh.spGroup, tr.item 
            
         union all 
        -- Faux Finalize
        select sh.spGroup,
               ts.item, 
               sum(ts.quantity) as qty, 
               sum(ts.extension) as extension
          from #Sales sh
          join tr_save ts on ts.sale_no = sh.sale_no
         group by sh.spGroup, item
       ) u
 group by spGroup, item

union all 
-- ===========================================
-- Sales by Staff
-- ===========================================
select u.spGroup,
       '' as PmtString,
	   0  as PmtTotal,
       '' as PcString,
       0  as Revenue,
       '' as DiscString,
       0  as DiscountAmount, 
       '' as dcString,
	   0  as dcTotal, 
       0  as taxAmount,
	   '' as itemString,
	   0  as itemTotal,
       cast(rtrim(op.first_name) + ' ' + rtrim(op.last_name) as char(25)) + cast(sum(extension) as char(10)) as opString,
	   cast(sum(extension) as numeric(18,2)) as opTotal,
	   '' as spString,
	   0  as spTotal,
	   '' as voidString,
	   0  as voidTotal 
  from (select sh.spGroup,
               tr.operator,
               sum(extension) as extension   
          from #Sales sh
          join transact tr on tr.sale_no = sh.sale_no
         where tr.department not in ('**TRANS**') 
         group by sh.spGroup, tr.operator
         union all 
        -- Faux Finalize
        select sh.spGroup,
               ts.operator, 
               sum(ts.extension) as extension
          from #Sales sh
          join tr_save ts on ts.sale_no = sh.sale_no
          join operator op on op.op_code = ts.operator
         group by sh.spGroup, ts.operator  
     ) u
  join operator op on op.op_code = u.operator
 group by u.spGroup, u.operator, op.first_name, op.last_name 
 
 union all
 
-- ===========================================
-- Sales by Salespoint
-- ===========================================
select u.spGroup,
       '' as PmtString,
	   0  as PmtTotal,
       '' as PcString,
       0  as Revenue,
       '' as DiscString,
       0  as DiscountAmount, 
       '' as dcString,
	   0  as dcTotal, 
       0  as taxAmount,
	   '' as itemString,
	   0  as itemTotal,
	   '' as opString,
	   0  as opTotal,
	   concat(salespoint,
		space(6),
		replicate(' ', 4-len(sum(sale_count))) + cast(sum(sale_count) as varchar),
		space(3),
		replicate(' ',5-len(cast(sum(extension)/(select sum(extension) from #Salespoints)*100 as decimal(4,1)))) + cast(cast(sum(extension)/(select sum(extension) from #Salespoints)*100 as decimal(4,1)) as varchar),
		space(3),
		replicate(' ',8-len(sum(extension))) + cast(sum(extension) as varchar)) as spString,
	   cast(sum(extension) as decimal(18,2)) as spTotal,
	   '' as voidString,
	   0  as voidTotal 
  from #Salespoints u
 group by u.spGroup, u.salespoint

union all
-- ===========================================
-- Voids by salespoint group
-- ===========================================
select sg.descrip, 
       '' as PmtString,
	   0  as PmtTotal,
       '' as PcString,
       0  as Revenue,
       '' as DiscString,
       0  as DiscountAmount, 
       '' as dcString,
	   0  as dcTotal,
	   0  as taxAmount, 
       '' as itemString,
	   0  as itemTotal, 
	   '' as opString,
	   0  as opTotal,
	   '' as spString,
	   0  as spTotal,  
	   concat(substring(replace(tag_line,'''',''),charindex(' of', tag_line)+4,33),
			  space(1),
			  substring(tag_line,charindex(': ',tag_line)+2,(charindex(' of', tag_line)-CHARINDEX(': ',tag_line))-1)
			 ) as voidString,
	   cast(substring(tag_line,charindex(': ',tag_line)+2,(charindex(' of', tag_line)-CHARINDEX(': ',tag_line))-1) as integer) as voidTotal
from sp_log sp
join operator op on op.op_code = sp.operator
join sp_link sl on sl.salespoint = sp.salespoint
join sp_group sg on sg.group_no = sl.group_no
where sp.date_time between '2018-08-14' and '2018-08-14 23:59:59' -- cast(getdate() as date) and getdate()
       and sg.group_no = @piSalespointGroup
	   and activ_type=1060 and tag_line like 'Void item:%'</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Revenue">
          <DataField>Revenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="spGroup">
          <DataField>spGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PmtString">
          <DataField>PmtString</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PmtTotal">
          <DataField>PmtTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="PcString">
          <DataField>PcString</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DiscString">
          <DataField>DiscString</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DiscountAmount">
          <DataField>DiscountAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="taxAmount">
          <DataField>taxAmount</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="dcString">
          <DataField>dcString</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="dcTotal">
          <DataField>dcTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="itemString">
          <DataField>itemString</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="opString">
          <DataField>opString</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="itemTotal">
          <DataField>itemTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="spString">
          <DataField>spString</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="opTotal">
          <DataField>opTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="spTotal">
          <DataField>spTotal</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="voidString">
          <DataField>voidString</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="voidTotal">
          <DataField>voidTotal</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="SalespointGroup">
      <Query>
        <DataSourceName>SiriusSQLPK_reporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pFBsp_group">
            <Value>=Parameters!pFBsp_group.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>set transaction isolation level read uncommitted
select rtrim(descrip) as spGroup
	, group_no
  from sp_group
where rtrim(descrip) in(@pFBsp_group)
order by descrip</CommandText>
      </Query>
      <Fields>
        <Field Name="spGroup">
          <DataField>spGroup</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="group_no">
          <DataField>group_no</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
