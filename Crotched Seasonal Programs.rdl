  <DataSets>
    <DataSet Name="getData">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@pStartDate">
            <Value>=Parameters!pStartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pEndDate">
            <Value>=Parameters!pEndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@pItem">
            <Value>=Parameters!pItem.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>set transaction isolation level read uncommitted

;with passes as 
(
select cast(t.date_time as date) as sale_date
	, t.department
	, t.category
	, t.item
	, t.trans_no
	, t.mastertran
	, ti.info_num as pass_no
from transact t
join tr_info ti on t.trans_no=ti.trans_no
		     and ti.info_type=20
join gst_pass gp on ti.info_num=gp.pass_no
			    and gp.voided_by=''
where (t.department='cm_seaspgm' and t.category='devo' and t.item in('AST_2','AST_AM_2','COM_2','COM_AM_2','FS_2','FUT_2','TIG_2','TIG_AM_2'))
	and t.date_time between @pStartDate and @pEndDate+' 23:59:59'
)
, passes_guest as
(
select p.*
	, dbo.proper(concat(rtrim(g.last_name),', ',rtrim(g.first_name))) as passholder
from passes p
join tr_info ti on p.trans_no=ti.trans_no
join guests g on ti.info_num=g.guest_no
		     and ti.info_type=10
)

select p.sale_date
	, d.descrip as department
	, c.descrip as category
	, i.descrip as item
	, p.pass_no
	, dbo.proper(t.item) as pass_modifier
	, p.passholder
from passes_guest p
join departme d on p.department=d.department
join category c on p.department=c.department
			   and p.category=c.category
join items i on p.department=i.department
			and p.category=i.category
			and p.item=i.item
left join transact t on p.trans_no=t.mastertran
					and t.mastertran&lt;&gt;t.trans_no
where i.descrip in(@pItem)</CommandText>
      </Query>
      <Fields>
        <Field Name="sale_date">
          <DataField>sale_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="department">
          <DataField>department</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="category">
