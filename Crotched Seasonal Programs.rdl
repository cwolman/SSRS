        </Field>
        <Field Name="passholder">
          <DataField>passholder</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="getItems">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>set transaction isolation level read uncommitted

select descrip
from items
where (department='cm_seaspgm' and category='devo' and item in('AST_2','AST_AM_2','COM_2','COM_AM_2','FS_2','FUT_2','TIG_2','TIG_AM_2')) 
order by descrip</CommandText>
      </Query>
      <Fields>
        <Field Name="descrip">
          <DataField>descrip</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
