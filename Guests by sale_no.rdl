  <DataSets>
    <DataSet Name="ds">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@sale_no">
            <Value>=Parameters!sale_no.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>set transaction isolation level read uncommitted

select g.guest_no
	, concat(rtrim(g.last_name),', ',rtrim(g.first_name)) as guest_name
	, dbo.cw_fn_encode128B(g.guest_no) as guest_no_barcode
	, count(g.guest_no) over () as record_count
from transact t 
join gst_pass gp on t.trans_no=gp.trans_no
join guests g on gp.guest_no=g.guest_no
where t.sale_no=@sale_no
order by g.last_name, g.first_name</CommandText>
      </Query>
      <Fields>
        <Field Name="guest_no">
          <DataField>guest_no</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="guest_name">
          <DataField>guest_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="guest_no_barcode">
          <DataField>guest_no_barcode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="record_count">
          <DataField>record_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
