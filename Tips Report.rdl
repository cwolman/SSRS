      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>55e7286c-1204-4c2d-93e0-abdee3e06cd1</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="SalespointGroups">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT CAST(sg.group_no AS CHAR(6)) AS SalespointIndex
	, RTrim(sg.descrip) AS descrip
FROM sp_group sg
ORDER BY RTrim(sg.descrip)</CommandText>
      </Query>
      <Fields>
        <Field Name="SalespointIndex">
          <DataField>SalespointIndex</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="descrip">
          <DataField>descrip</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Salespoints">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@salespointGroups">
            <Value>=Parameters!salespointGroups.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT RTRIM(sp.salespoint) as SalespointIndex
	, RTrim(sp.salespoint) AS descrip
FROM sales_pt sp
join sp_link sl on sl.salespoint=sp.salespoint
WHERE sp.hidden = 0 and sl.group_no in(@salespointGroups)
ORDER BY descrip</CommandText>
      </Query>
      <Fields>
        <Field Name="SalespointIndex">
          <DataField>SalespointIndex</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="descrip">
          <DataField>descrip</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="reportData">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@salespoints">
            <Value>=Parameters!salespoints.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@operators">
            <Value>=Parameters!operators.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @sDate as datetime=concat(cast(@startDate as date),' 05:00:00')
declare @eDate as datetime=concat(dateadd(d, 1, cast(@endDate as date)),' 04:59:59')

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED

select *
	, case when rn=1 then net_sales_less_tips else 0 end as op_total
	, case when rn=1 then tips else 0 end as op_tips
from
(
select s.dte
	, concat(rtrim(dbo.proper(o.last_name)),', ',rtrim(dbo.proper(o.first_name))) as operator
	, sum(net_sales-tips) as net_sales_less_tips
	, sum(tips) as tips
	, dbo.proper(descrip) as payment_type
	, sum(pmt_amt) as pmt_amt
	, row_number() over (partition by s.dte, concat(rtrim(dbo.proper(o.last_name)),', ',rtrim(dbo.proper(o.first_name))) order by s.dte, concat(rtrim(dbo.proper(o.last_name)),', ',rtrim(dbo.proper(o.first_name)))) as rn
from
(
-- sales
select cast(date_time as date) as dte
	, salespoint
	, operator
	, sum(extension+disc_amt) as gross_sales
	, sum(disc_amt) as discounts
	, sum(extension-(tax_amount+tax_amt2)) as net_sales
	, sum(case when pr_ctr_1=1 then pcsplit_1 else 0 end +
	  case when pr_ctr_2=1 then pcsplit_2 else 0 end +
	  case when pr_ctr_3=1 then pcsplit_3 else 0 end +
	  case when pr_ctr_4=1 then pcsplit_4 else 0 end +
	  case when pr_ctr_5=1 then pcsplit_5 else 0 end +
	  case when pr_ctr_6=1 then pcsplit_6 else 0 end) as tips
from transact
where date_time between @sDate and @eDate
		and salespoint in(@salespoints)
		and operator in(@operators)
		and department&lt;&gt;'**TRANS**'
group by cast(date_time as date), salespoint, operator
) s

join
 
-- payments
(
select cast(sh.date_time as date) as dte, sh.salespoint, sh.operator, pt.descrip, sum(pays.pmt_amt) as pmt_amt
from sale_hdr sh
cross apply (values	
				  (pmt_type1, pmt_amt1)
				, (pmt_type2, pmt_amt2) 
				, (pmt_type3, pmt_amt3)
				, (pmt_type4, pmt_amt4)
			)pays (pmt_type, pmt_amt)
join pmt_type pt on pt.pmtype_no=pays.pmt_type
where date_time between @sDate and @eDate
		and salespoint in(@salespoints)
		and operator in(@operators)
		and pays.pmt_amt&lt;&gt;0
group by cast(sh.date_time as date), sh.salespoint, sh.operator, pt.descrip
) p
on s.dte=p.dte and s.salespoint=p.salespoint and s.operator=p.operator
join operator o on o.op_code=s.operator
group by s.dte, concat(rtrim(dbo.proper(o.last_name)),', ',rtrim(dbo.proper(o.first_name))), p.descrip
)final
order by dte, operator, payment_type</CommandText>
      </Query>
      <Fields>
        <Field Name="dte">
          <DataField>dte</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="operator">
          <DataField>operator</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="net_sales_less_tips">
          <DataField>net_sales_less_tips</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="tips">
          <DataField>tips</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="payment_type">
          <DataField>payment_type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="pmt_amt">
          <DataField>pmt_amt</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="rn">
          <DataField>rn</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="op_total">
          <DataField>op_total</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="op_tips">
          <DataField>op_tips</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Operators">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@startDate">
            <Value>=Parameters!startDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@endDate">
            <Value>=Parameters!endDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@salespoints">
            <Value>=Parameters!salespoints.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct t.operator, concat(rtrim(o.last_name),', ',rtrim(o.first_name)) as name
from transact t
join operator o on t.operator=o.op_code
where t.date_time between @startDate and dateadd(s, -1, @endDate+1)
	and t.salespoint in(@salespoints)
	and (t.pr_ctr_1=1 or t.pr_ctr_2=1 or t.pr_ctr_3=1 or t.pr_ctr_4=1 or t.pr_ctr_5=1 or t.pr_ctr_6=1)</CommandText>
      </Query>
      <Fields>
        <Field Name="operator">
          <DataField>operator</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="name">
          <DataField>name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
